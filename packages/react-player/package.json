{
  "private": true,
  "name": "react-player",
  "version": "0.0.1",
  "description": "Rui application player in React.js",
  "main": "lib/index.js",
  "scripts": {
    "build": "remix build",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "proton": "ts-node-esm --project ./tsconfig-node.json -r dotenv/config -r tsconfig-paths/register ./app/tools/run-proton.ts",
    "update-rpd-models": "ts-node-esm --project ./tsconfig-node.json -r dotenv/config -r tsconfig-paths/register ./app/tools/update-rpd-models.ts",
    "test": "echo \"Error: no test specified\""
  },
  "keywords": [],
  "author": "Fossil",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "4.8.1",
    "@remix-run/express": "^1.7.2",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@ruiapp/antd-rocks": "workspace:^0.0.1",
    "@ruiapp/echarts-extension": "workspace:^0.0.1",
    "@ruiapp/move-style": "workspace:^0.0.1",
    "@ruiapp/monaco-extension": "workspace:^0.0.1",
    "@ruiapp/react-designer": "workspace:^0.0.1",
    "@ruiapp/react-rapid-rocks": "workspace:^0.0.1",
    "@ruiapp/react-renderer": "workspace:^0.0.1",
    "@ruiapp/react-rocks": "workspace:^0.0.1",
    "antd": "4.24.13",
    "axios": "^1.1.2",
    "axios-cookiejar-support": "^4.0.6",
    "chokidar": "^3.5.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.3",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "monaco-editor": "0.34.1",
    "morgan": "^1.10.0",
    "pluralize": "^8.0.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tough-cookie": "^4.1.2"
